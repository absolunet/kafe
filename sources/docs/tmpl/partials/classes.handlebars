<header class="class">
	<h1>{{name}}</h1>
	<h2>class</h2>
	{{{classDescription}}}
	<footer>Source: <a href="{{foundAt}}">{{{file}}}</a></footer>
</header>

{{#if properties}}
<section>
	<h1>Properties</h1>
	{{#properties}}
		{{>props}}
	{{/properties}}
</section>
{{/if}}

{{#if methods}}
<section>
	<h1>Methods</h1>
	{{#methods}}
		{{>method}}
	{{/methods}}
</section>
{{/if}}